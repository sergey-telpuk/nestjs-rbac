{
  "name": "nestjs-rbac",
  "version": "1.3.0",
  "description": "Rbac for NestJs",
  "author": "Sergey Telpuk",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "test:watch": "jest --watch  --config ./test/jest.json",
    "test:cov": "jest --coverage --config ./test/jest.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./test/jest.json",
    "test:e2e": "jest  --verbose  --config ./test/e2e/jest-e2e.json",
    "test:int": "jest  --verbose --config ./test/int/jest-int.json",
    "test": "jest --verbose --config ./test/jest.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergey-telpuk/nestjs-rbac.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@nestjs/common": "8.2.2",
    "@nestjs/core": "^8.2.2",
    "@nestjs/platform-express": "^8.2.2",
    "node-cache": "^5.1.2",
    "rimraf": "^3.0.0",
    "rxjs": "^7.4.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.2.2",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjsx/crud": "^4.6.2",
    "@nestjsx/crud-typeorm": "^4.6.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.9",
    "@types/supertest": "^2.0.10",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.13.1",
    "jest": "^27.3.1",
    "prettier": "^2.2.1",
    "sqlite3": "^4.2.0",
    "supertest": "^6.1.1",
    "ts-jest": "^27.0.7",
    "tsc-watch": "^4.2.9",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typeorm": "^0.2.30",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "keywords": [
    "rbac",
    "nestjs"
  ]
}
